index 55045bbad..8e4b62d99 100644
--- a/init/Kconfig
+++ b/init/Kconfig
@@ -1369,7 +1369,6 @@ config CC_OPTIMIZE_FOR_PERFORMANCE
 
 config CC_OPTIMIZE_FOR_PERFORMANCE_O3
 	bool "Optimize more for performance (-O3)"

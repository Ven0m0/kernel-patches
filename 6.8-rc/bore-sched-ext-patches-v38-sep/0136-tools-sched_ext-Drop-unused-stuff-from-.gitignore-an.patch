From 3923244db7eaa728f98bbb014ca113be78850692 Mon Sep 17 00:00:00 2001
From: Tejun Heo <tj@kernel.org>
Date: Fri, 23 Feb 2024 09:20:55 -1000
Subject: [PATCH 136/152] tools/sched_ext: Drop unused stuff from .gitignore
 and Makefile

---
 tools/sched_ext/.gitignore | 4 ----
 tools/sched_ext/Makefile   | 5 -----
 2 files changed, 9 deletions(-)

diff --git a/tools/sched_ext/.gitignore b/tools/sched_ext/.gitignore
index 215ed36b2..00e0eef67 100644
--- a/tools/sched_ext/.gitignore
+++ b/tools/sched_ext/.gitignore
@@ -1,9 +1,5 @@
 scx_simple
 scx_qmap
-scx_central
-scx_pair
-scx_flatcg
-scx_userland
 *.skel.h
 *.subskel.h
 /tools/
diff --git a/tools/sched_ext/Makefile b/tools/sched_ext/Makefile
index 731bf6278..626782a21 100644
--- a/tools/sched_ext/Makefile
+++ b/tools/sched_ext/Makefile
@@ -93,11 +93,6 @@ CFLAGS += -g -O2 -rdynamic -pthread -Wall -Werror $(GENFLAGS)			\
 	  -I$(INCLUDE_DIR) -I$(GENDIR) -I$(LIBDIR)				\
 	  -I$(TOOLSINCDIR) -I$(APIDIR) -I$(CURDIR)/include
 
-CARGOFLAGS := --release --target-dir $(OUTPUT_DIR)
-ifneq ($(CARGO_OFFLINE),)
-CARGOFLAGS += --offline
-endif
-
 # Silence some warnings when compiled with clang
 ifneq ($(LLVM),)
 CFLAGS += -Wno-unused-command-line-argument
